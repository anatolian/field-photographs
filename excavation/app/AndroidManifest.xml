<?xml version="1.0" encoding="utf-8" standalone="no"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.appenginedemo"
    android:installLocation="auto"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk android:minSdkVersion="8" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo1"
        android:theme="@android:style/Theme.Light.NoTitleBar"
        android:largeHeap="true"
        android:vmSafeMode="true"
        android:allowClearUserData="true"
        android:hardwareAccelerated="true" >
        <activity
            android:name=".SplashmainScreen"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.appenginedemo.MainActivity"
                   android:label="SplashmainScreen" 
                   
            android:configChanges="orientation|keyboard|keyboardHidden" >
        </activity>
        <activity
            android:name="com.appenginedemo.ActivityCamera"
             android:label="ActivityCamera" 
            android:configChanges="orientation|keyboard|keyboardHidden|screenLayout" >
        </activity>
        <activity
            android:name="com.appenginedemo.ActivityBase"
             android:label="ActivityBase" 
            android:configChanges="orientation|keyboard|keyboardHidden" >
        </activity>
        <activity
            android:name="com.appenginedemo.ActivityCamera1"
             android:label="ActivityCamera1" 
            android:configChanges="orientation|keyboard|keyboardHidden|screenLayout" >
        </activity>
        <activity
            android:name="com.appenginedemo.Activity_3d"
             android:label="Activity_3d" >
        </activity>
        <activity
            android:name="com.appenginedemo.MultiPhotoSelectActivity"
            android:label="MultiSelectActivity"
            android:configChanges="orientation|keyboard|keyboardHidden|screenLayout" >
        </activity>
        <activity
            android:name="com.appenginedemo.Activity_Sample"
            android:label="Activity_Sample"
            android:configChanges="orientation|keyboard|keyboardHidden|screenLayout" >
        </activity>
       <activity android:name="com.appenginedemo.ActivityImageProperty"
           android:label="Activity_Image_Property"
           android:configChanges="orientation|keyboard|keyboardHidden|screenLayout" >
           >
           
           
       </activity>
    </application>

</manifest>